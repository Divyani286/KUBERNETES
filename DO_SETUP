sudo snap install doctl -y
doctl auth init
sudo snap connect doctl:kube-config
echo $KUBECONFIG
mkdir .kube
doctl kubernetes cluster kubeconfig  save <NEW TOKEN ID>
cd ~/.kube && kubectl --kubeconfig="config" get nodes
export KUBECONFIG=~/.kube/config
kubectl get ns -A
kubectl create ns mi7-ns
kubectl get ns -A
